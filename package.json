        "@typescript-eslint/parser": "^2.34.0",
        "autoprefixer": "^9.8.4",
        "babel-eslint": "^10.1.0",
        "babel-loader": "^8.0.6",
        "clean-webpack-plugin": "^3.0.0",
        "compression-webpack-plugin": "^3.1.0",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.6.0",
        "eslint": "^6.8.0",

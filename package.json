    },
    "main": "./src/index.js",
    "scripts": {
        "dev": "npm run build_serv",
        "build_serv": "node $npm_package_config_index -b -s",
        "build_serv:prod": "npm run build:prod && npm run pm2",
        "build": "node $npm_package_config_index -b",
        "build:prod": "npm run build -- -p",
        "serv": "node $npm_package_config_index -s",
        "serv:prod": "npm run serv -- -p",
        "deploy": "npm ci && node ./node_modules/siegel/src/scripts/install_peers && npm run build_serv:prod",
        "pm2": "./node_modules/.bin/pm2 start $npm_package_config_index -- -s -p",
        "validate": "./node_modules/.bin/tsc || ./node_modules/.bin/eslint ./",
        "with_args": "node $npm_package_config_index"
    },

    "url": "https://github.com/CyberCookie/siegel",
    "bugs": "https://github.com/CyberCookie/siegel/issues",
    "description": "Web application development ecosystem",
    "keywords": [
        "web",
        "create app",
        "web application",
        "typescript",
        "react",
        "reactjs",
        "node",

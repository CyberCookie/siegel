{
    "name": "oswell_client",
    "description": "oswell template",
    "version": "0.5.0",
    "url": "",
    "scripts": {
        "dev": "npm run build_serv",
        "build_serv": "node ./server/index.js -b -s",
        "build_serv:prod": "npm run build:prod && npm run pm2",
        "build": "node ./server/index.js -b",
        "build:prod": "npm run build -- -p",
        "serv": "node ./server/index.js -s",
        "serv:prod": "npm run serv -- -p",
        "deploy": "npm i && npm run build_serv:prod",
        "pm2": "./node_modules/.bin/pm2 start ./server/index.js -- -s -p",
        "validate": "./node_modules/.bin/tsc --project || ./node_modules/.bin/eslint ./",
        "with_args": "node ./server/index.js"
    },
    "dependencies": {
        "connect-history-api-fallback": "^1.6.0",
        "express": "^4.17.1",
        "express-static-gzip": "^2.0.7",

{
    "compilerOptions": {
        "lib": [ "dom", "dom.iterable", "esnext" ],
        "outDir": "../lib_client",
        "jsx": "react",
		"baseUrl": "client_core",
		"target": "es2020",
        "module": "es2020",
		"moduleResolution": "node",
        "allowJs": true,
		"esModuleInterop": true,
		"strict": true,
		"noImplicitAny": true,
		"skipLibCheck": true,
        "declaration": true,
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "isolatedModules": true
    },
    "include": ["../global.d.ts", "."]
}
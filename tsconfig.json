{
    "compilerOptions": {
		"target": "esnext",
		"moduleResolution": "node",
		"module": "esnext",
		"allowJs": true,
		"noEmit": true,
		"noEmitHelpers": true,
		"strict": true,
		"esModuleInterop": true,
		"jsx": "react",
		"baseUrl": "./",
		"paths": {
			"core/*": ["./oswell_ui_dev_core/ui_core/*"],
			"core-store/*": ["./oswell_ui_dev_core/ui_core/store/*"],
			"core-ui/*": ["./oswell_ui_dev_core/ui_core/ui/*"],
			"core-utils/*": ["./oswell_ui_dev_core/ui_core/utils/*"],
			"app/*": ["./client/main/*"]
		}
	},
    "include": [
		"./oswell_ui_dev_core",
		"./client",
		"./server"
	]
}  